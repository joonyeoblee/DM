				WorldViewDirection = SafeNormalize( WorldViewDirection );

				float2 uv_WallTex = IN.ase_texcoord8.xy * _WallTex_ST.xy + _WallTex_ST.zw;
				float2 uv_MainTex149 = IN.ase_texcoord8.xy;
				float4 tex2DNode149 = tex2D( _MainTex, uv_MainTex149 );
				float2 texCoord164 = IN.ase_texcoord8.xy * half2( 0,-0.1 ) + ( half2( 0.015625,0 ) * _ColorSelect );
				float4 lerpResult152 = lerp( ( _WallBrightness * tex2D( _WallTex, uv_WallTex ) ) , ( tex2DNode149.r * tex2D( _ColorTheme, texCoord164 ) ) , tex2DNode149.g);
				
				float2 uv_GlassNM = IN.ase_texcoord8.xy * _GlassNM_ST.xy + _GlassNM_ST.zw;
				
				float3 Albedo = ( lerpResult152 * _Color ).rgb;
				float3 Normal = UnpackNormalScale( tex2D( _GlassNM, uv_GlassNM ), 1.0f );
				float3 Emission = 0;
				float3 Specular = 0.5;
				float Metallic = 0;
				float Smoothness = ( tex2DNode149.r * _SmoothnessValue );
				float Occlusion = 1;
