				float MaskBlue70 = tex2DNode7.b;
				float2 texCoord82 = IN.ase_texcoord8.zw * float2( 1,1 ) + ( half2( 0.015625,0 ) * _ColorSelect );
				float4 ColorUV396 = tex2D( _MainTex, texCoord82 );
				float2 appendResult88 = (float2((float)( _ColorSelect * 0 ) , -0.5));
				float2 texCoord89 = IN.ase_texcoord8.zw * float2( 1,1 ) + appendResult88;
				float4 DirtUV395 = tex2D( _MainTex, texCoord89 );
				float3 temp_cast_1 = (( tex2DNode7.g * _DirtMultiplier )).xxx;
				float3 temp_cast_2 = (( 1.0 - IN.ase_color.r )).xxx;
				float DirtHeight14 = saturate( ( 1.0 - ( ( distance( temp_cast_1 , temp_cast_2 ) - _DirtRange ) / max( _DirtSmooth , 1E-05 ) ) ) );
				float4 lerpResult23 = lerp( ( MaskBlue70 * ( 1.0 - ( _InsulationDarkness * ( 1.0 - IN.ase_color.g ) ) ) * ColorUV396 ) , ( DirtUV395 * tex2DNode7.r ) , ( DirtHeight14 * _DirtOpacity ));
				float4 Albedo64 = lerpResult23;
				
				float2 uv_Normals = IN.ase_texcoord8.xy * _Normals_ST.xy + _Normals_ST.zw;
				float3 unpack3 = UnpackNormalScale( tex2D( _Normals, uv_Normals ), _NormalsScale );
				unpack3.z = lerp( 1, unpack3.z, saturate(_NormalsScale) );
				float3 Normals59 = unpack3;
				
				float VertexGreen67 = IN.ase_color.g;
				clip( tex2DNode7.b - ( VertexGreen67 * _Mask ));
